<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #99ccff;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cafe343.AppetizerController">
   <children>
      <Label layoutX="330.0" layoutY="14.0" prefHeight="82.0" prefWidth="365.0" text="Appetizers">
         <font>
            <Font name="Sitka Text" size="72.0" />
         </font>
      </Label>
      <Button fx:id="returnButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleReturnButton" prefHeight="50.0" prefWidth="150.0" text="Return" />
      <Button fx:id="orderHistoryButton" layoutX="14.0" layoutY="583.0" mnemonicParsing="false" onAction="#handleOrderHistoryButton" prefHeight="50.0" prefWidth="150.0" text="Order History" />
      <Button fx:id="checkoutButton" layoutX="860.0" layoutY="583.0" mnemonicParsing="false" onAction="#handleCheckoutButton" prefHeight="50.0" prefWidth="150.0" text="Checkout" />
      <Button fx:id="serverButton" layoutX="860.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleServerButton" prefHeight="50.0" prefWidth="150.0" text="Server" />
      <TableView fx:id="tableViewAppetizers" layoutX="112.0" layoutY="96.0" prefHeight="450.0" prefWidth="800.0">
        <columns>
          <TableColumn prefWidth="150.0" text="Name">
                               <cellValueFactory>
                                  <PropertyValueFactory property="menuObjectNameProperty" />
                              </cellValueFactory>
                          </TableColumn>
          <TableColumn prefWidth="150.0" text="Price" >
                                <cellValueFactory>
                                  <PropertyValueFactory property="menuObjectPriceProperty" />
                              </cellValueFactory>
                          </TableColumn>
                                
        </columns>
      </TableView>
   </children>
</AnchorPane>
